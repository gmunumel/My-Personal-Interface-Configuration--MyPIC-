!-----------------------------------------------------------------------
! file: ~/.Xdefaults
! author: leprosys
!-----------------------------------------------------------------------
 
!{{{ XFT
!Xft.dpi: 96
Xft.antialias: 1
Xft.hinting: 1
Xft.hintstyle: hintfull
Xft.rgba: rgb
!}}}
 
 
! {{{ Xcursor
!Xcursor*theme : Vanilla-DMZ-AA
!Xcursor*size : 10
! }}}
 
 
! {{{ URxvt
!URxvt*geometry : 124x124
!URxvt*termName : rxvt-256color
!URxvt*imLocale : en_US.UTF-8
URxvt*saveLines : 32767
 
URxvt*scrollBar : false
URxvt*internalBorder : 0
URxvt*secondaryScroll : true
 
URxvt*depth : 32
URxvt*buffered : true
URxvt*transparent : true
URxvt.foreground : white
URxvt*shading : 20
URxvt.tintColor: #000000
!#URxvt*fading : 10
 
URxvt*cursorUnderline : true
URxvt*cursorBlink : true
 
!URxvt*urgentOnBell : true
 
Urxvt*font : -*-dina-medium-*-*-*-13-*-*-*-*-*-*-*
URxvt*boldFont : -*-dina-bold-*-*-*-13-*-*-*-*-*-*-*
!URxvt.font: xft:Monospace:pixelsize=10:antialias=true
!URxvt*italicFont : -*-dina-medium-i-normal-*-12-*-*-*-*-*-iso8859-1
!URxvt*boldItalicFont : -*-dina-bold-i-normal-*-12-*-*-*-*-*-iso8859-1
 
!URxvt*font : xft:DejaVu Sans Mono:antialias=true:pixelsize=13
!URxvt*boldFont : xft:DejaVu Sans 
!Mono:style=bold:antialias=true:pixelsize=12
!-----1-
URxvt*italicFont : xft:DejaVu Sans 
!-----2--
!Mono:style=italic:antialias=true:pixelsize=12
!-----1----
URxvt*boldItalicFont : xft:DejaVu Sans 
Mono:style=bolditalic:antialias=true:pixelsize=12
!-----2---
 
!URxvt*perl-lib : /usr/lib/urxvt/perl/
!URxvt*perl-ext-common : default,matcher,searchable-scrollback,tabbed
!URxvt*urlLauncher : /usr/bin/swiftweasel
!URxvt*matcher*button : 1
!URxvt*matcher*pattern.1 : \\bwww\\.[\\w-]+\\.[\\w./?&@#-]*[\\w/-]
 
! }}}
 
 
!{{{ Xterm
xterm*internalBorder: 0
xterm*loginShell: true
xterm*scrollBar: false
xterm*cursorBlink: false
xterm*geometry: 110x30
xterm*saveLines: 65535
